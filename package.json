{
  "name": "calver-ci",
  "version": "22.9.13-rc.30",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "calver-ci": "node -r ts-eager/register src/index.ts",
    "lint": "eslint src",
    "lint:fix": "concurrently \"yarn:lint --fix\"",
    "prettier": "prettier src --check",
    "prettier:fix": "concurrently \"yarn:prettier --write\"",
    "format": "concurrently \"yarn:prettier:fix\" \"yarn:lint:fix\"",
    "clean": "cf dist",
    "build": "concurrently --group \"yarn:clean\" \"yarn:build:preparebuild\" \"yarn:build:ts\"",
    "build:preparebuild": "yarn rebuild",
    "build:ts": "node -r ts-eager/register src/build.ts",
    "typecheck": "tsc --noEmit",
    "usage": "./scripts/usage.sh"
  },
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/busla/calver-ci.git"
  },
  "keywords": [
    "calver",
    "calendar",
    "versioning",
    "semver",
    "semantic",
    "versioning",
    "versioning",
    "cli"
  ],
  "author": {
    "name": "JÃ³n Levy",
    "email": "nonni@nonni.cc"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/busla/calver-ci/issues"
  },
  "homepage": "https://github.com/busla/calver-ci#readme",
  "devDependencies": {
    "@types/node": "18.7.16",
    "@types/slug": "5.0.3",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "@yarnpkg/esbuild-plugin-pnp": "3.0.0-rc.15",
    "clear-folder": "3.2.1",
    "concurrently": "7.4.0",
    "esbuild": "0.15.7",
    "esbuild-darwin-64": "0.15.7",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.7.1",
    "ts-eager": "2.0.2",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "arg": "^5.0.2",
    "calver": "^22.8.4"
  },
  "devDependenciesMeta": {
    "esbuild-darwin-64@(": {
      "unplugged": true
    }
  },
  "peerDependenciesMeta": {
    "esbuild-darwin-64": {
      "optional": false
    },
    "esbuild-darwin-arm64": {
      "optional": false
    },
    "esbuild-linux-64": {
      "optional": false
    },
    "esbuild-linux-arm64": {
      "optional": false
    },
    "esbuild-windows-64": {
      "optional": false
    },
    "esbuild-windows-arm64": {
      "optional": false
    }
  }
}
